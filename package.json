{
  "name": "onsen-tabi-web",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": "20.x"
  },
  "scripts": {
    "start": "vite",
    "build": "tsc --noEmit && vite build",
    "serve": "vite preview",
    "test": "vitest run",
    "coverage": "vitest run --coverage",
    "test:codecov:analytics": "vitest --reporter=junit --outputFile=test-report.junit.xml",
    "lint": "eslint . --ext .ts,.tsx src/",
    "lintfix": "npm run lint -- --fix",
    "format": "prettier --write \"**/*.+(js|json|yml|ts|tsx)\"",
    "sentry:sourcemaps": "npx sentry-cli sourcemaps inject --org onsen-tabi --project web ./build && npx sentry-cli sourcemaps upload --org onsen-tabi --project web ./build",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "chromatic": "npx chromatic --exit-zero-on-changes"
  },
  "devDependencies": {
    "@codecov/vite-plugin": "^1.9.1",
    "@ianvs/prettier-plugin-sort-imports": "^4.4.2",
    "@sentry/cli": "^2.46.0",
    "@sentry/react": "^9.33.0",
    "@storybook/addon-actions": "^8.1.11",
    "@storybook/addon-essentials": "^8.1.11",
    "@storybook/addon-interactions": "^8.1.11",
    "@storybook/addon-links": "^8.1.11",
    "@storybook/addon-onboarding": "8.1.11",
    "@storybook/blocks": "^8.1.11",
    "@storybook/react": "^8.1.11",
    "@storybook/react-vite": "^8.1.11",
    "@storybook/test": "^8.1.11",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/eslint": "^9.6.1",
    "@types/node": "^20.11.30",
    "@types/react": "^18.2.74",
    "@types/react-dom": "^18.2.24",
    "@types/react-helmet": "^6.1.11",
    "@types/testing-library__user-event": "^4.2.0",
    "@typescript-eslint/eslint-plugin": "^8.35.0",
    "@typescript-eslint/parser": "^8.35.0",
    "@vitejs/plugin-react": "^4.6.0",
    "@vitest/coverage-v8": "^3.2.4",
    "axios": "^1.10.0",
    "chromatic": "^11.2.0",
    "eslint": "^9.30.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-n": "^17.20.0",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-storybook": "^9.0.14",
    "globals": "^16.2.0",
    "happy-dom": "^18.0.1",
    "jwt-decode": "^4.0.0",
    "prettier": "^3.6.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-ga4": "^2.1.0",
    "react-helmet": "^6.1.0",
    "react-router-dom": "^6.22.3",
    "react-select": "^5.8.1",
    "react-select-event": "^5.5.1",
    "react-use": "^17.5.0",
    "storybook": "^8.1.11",
    "storybook-addon-react-router-v6": "^2.0.15",
    "styled-components": "^6.1.8",
    "typescript": "^5.4.4",
    "vite": "^7.0.0",
    "vitest": "^3.2.4",
    "vitest-sonar-reporter": "^2.0.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "packageManager": "yarn@4.1.0"
}
